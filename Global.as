package {    public class Global {                private var _data:Object;            private static var _self:Global;            public function Global() {                _data = new Object();            }            public static function getInstance():Global {                if(_self == null) {                    _self = new Global();                }                return _self;            }            public function setValue(key:String, value:*):void {                _data[key] = value;            }            public function getValue(key:String):* {                return _data[key];            }    }}